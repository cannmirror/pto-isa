<?xml version="1.0" encoding="UTF-8"?>
<config name="pto_isa" copy_type="delivery" src_path="include/" dst_path="pto_isa" install_type="all" install_mod="550" install_own="$username:$usergroup">
    <package_info>
        <product_name>cann</product_name>
        <func_name>pto-isa</func_name>
        <suffix>run</suffix>
        <install_script>share/info/pto_isa/scripts/install.sh</install_script>
        <help>share/info/pto_isa/scripts/help.info</help>
        <version>package/pto_isa/version.xml</version>
        <cleanup>share/info/pto_isa/script/cleanup.sh</cleanup>
        <parallel>true</parallel>
    </package_info>
    <generate_info value="pto_isa_version.h" dst_path="share/info/pto_isa" install_path="$(TARGET_ENV)/include/version" install_mod="440" install_type="devel" generator="version_header">
       <pto_isa_VERSION>$(ASCEND_VER)</pto_isa_VERSION>
       <pto_isa_TIMESTAMP>$(TIMESTAMP_NO)</pto_isa_TIMESTAMP>
    </generate_info>
    <generate_info value="scene.info" dst_path="share/info/pto_isa" install_path="share/info/pto_isa" install_mod="440">
        <os>$(OS_NAME)</os>
        <os_version>$(OS_VER)</os_version>
        <arch>$(ARCH)</arch>
    </generate_info>
    <file_info value="pto_isa_script" copy_type="source" src_path="" dst_path="share/info/pto_isa" install_path="share/info/pto_isa">
        <file value="script" entity="true"/>
        <file value="version.info" dst_path="share/info/pto_isa" install_path="share/info/pto_isa"/>
    </file_info>
    <file_info copy_type="source" src_path="" dst_path="share/info/pto_isa/include/pto/" install_path="$(TARGET_ENV)/include/pto" install_type="all">
        <file value="common" install_mod="550" entity="true"/>
        <file value="npu" install_mod="550" entity="true"/>
        <file value="cpu" install_mod="550" entity="true"/>
        <file value="pto-inst.hpp" install_mod="550" entity="true"/>
    </file_info>
    <file_info copy_type="source" src_path="" dst_path="share/info/pto_isa/include/pto/npu" install_path="$(TARGET_ENV)/include/pto/npu" install_type="all">
        <file value="a2a3" install_mod="550" entity="true"/>
        <file value="a5" install_mod="550" entity="true"/>
    </file_info>
    <block_info dependtree="false" dst_path="share/info/pto_isa" block_conf_path="ascend">
        <block name="EngineeringCommon"/>
        <block name="EngineeringFiles"/>
    </block_info>
    <dir_info value="pto_isa" install_path="" install_mod="750" install_type="all">
        <path value="share" install_mod="750"/>
        <path value="share/info" install_mod="750"/>
        <path value="share/info/pto_isa" install_mod="550"/>
        <path value="share/info/pto_isa/script" install_mod="550"/>
        <path value="$(TARGET_ENV)/include"/>
        <path value="$(TARGET_ENV)/include/pto"/>
        <path value="$(TARGET_ENV)/include/pto/common"/>
        <path value="$(TARGET_ENV)/include/pto/npu"/>
        <path value="$(TARGET_ENV)/include/pto/npu/a2a3"/>
        <path value="$(TARGET_ENV)/include/pto/npu/a5"/>
        <path value="$(TARGET_ENV)/include/pto/cpu"/>
    </dir_info>
</config>
